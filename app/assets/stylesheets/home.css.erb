/***** MODAL CONTACT *****/

.modal-content {
  width:400px !important;
  margin-left:80px;
  border-radius: 0px;
}

.modal-header {
  color: #e39938;
  font-family: "Lato";
  border:none !important;
  text-align:center;
  font-size:40px !important;
  text-transform:uppercase;
}

.modal-title {
  font-size:30px ;
}

.modal-body {
    font-family: "Lato";
    font-weight: 100;
    font-size: 16px;
    color: #555;
}


/* Style for text modal */


.modal-body p {
  color: rgba(255,255,255,0.5);
  font-weight: 300;
  font-size: 1.15em;
  line-height: 1.4;
}


/* Style for form modal */
.modal-body form {
  position: relative;
  margin: 0 auto;
padding: 10px 30px;
  text-align: left;
}




.modal-body label {
  display: block;
  padding: 10px 0 0;
  color: #365485;
  text-transform: uppercase;
  letter-spacing: 1px;
  font-size:10px;
  font-weight: bold;
  line-height:10px;

}

.modal-body input[type="text"],
.modal-body input[type="password"] {
  border: 2px solid #6d91a3;
  background: transparent;
  font-weight:100 !important;
  color: #313131;
  font-size: 1.5em;
}

.modal-body input[type="text"]:focus,
.modal-body input[type="password"]:focus {
  border-color: #ef7e7e;
  font-size: 1.5em;

  color: #313131;
}


.modal-body button {
  display: block;
  margin: 10px auto;
  border: none;
  background: #2e5a8a;
  color: #fff;
  text-transform: uppercase;
  letter-spacing: 1px;
  font-weight: 800;
  font-size: 20px;
}

.modal-body button:hover {
  background: #4391c5;
  color: #fff;

}




@media screen and (max-width: 770px) {
  .content-style-overlay {
